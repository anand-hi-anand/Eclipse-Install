<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="
        http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd">

    <!-- bean definitions here -->
    <bean class="in.sp.beans.Forces" id="SF1">
         <property name="unit" value="Para SF"/>
         <property name="armedWing" value="Army"/>   
         <property name="country" value="India"/> 
    </bean>

    <bean class="in.sp.beans.Forces" id="SF2">
         <property name="unit" value="SAS"/>
         <property name="armedWing" value="Army"/>   
         <property name="country" value="Britain"/> 
    </bean>

</beans>