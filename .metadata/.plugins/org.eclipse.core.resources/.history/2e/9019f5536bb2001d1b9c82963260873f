class test {
	
	public static void main (String [] args) {
		
		ArrayList<Integer> p = new ArrayList<>();
		
		p.add(9);
		p.add(10);
		p.add(878);
		p.add(567);
		p.add(1);
		
		Collections.sort(p);
		
		ListIterator l = p.listIterator();
		
		while (l.hasNext()) {
			
			System.out.print(l.next()+ " ");
		}
		
		System.out.println();
		System.out.println("Iteration Finished \n");
		
		
		
		
	}
	
	
}